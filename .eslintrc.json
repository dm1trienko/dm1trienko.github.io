{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "space-before-function-paren": "off",
    "comma-dangle": "off",
    "no-undef": "off",
    "no-useless-escape": "off",
    "quotes": "off",
    "no-unused-vars": "off",
    "no-multiple-empty-lines": "off",
    "no-case-declarations": "off",
    "no-new": "off"
  }
}
